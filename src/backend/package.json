		"axios": "^1.1.3",
		"bootstrap": "^5.1.3",
		"datatables.net": "^1.13.1",
		"datatables.net-bs4": "^1.13.1",
		"datatables.net-bs5": "^1.13.1",
		"datatables.net-buttons": "^2.3.3",
		"datatables.net-buttons-dt": "^2.3.3",
		"datatables.net-dt": "^1.12.1",
		"feather-icons": "^4.29.0",
		"html2pdf.js": "^0.10.1",
		"jquery": "^3.6.0",
		"marked": "^4.2.1",
		"parcel": "^2.8.1",
