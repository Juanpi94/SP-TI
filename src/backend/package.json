{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "static/dist/main.js",
	"css": "static/dist/styles.css",
	"targets": {
		"default": {
			"optimize": true,
			"distDir": "./static/dist"
		},
		"main": {
			"optimize": true,
			"source": "./static/src/js/**/*.js"
		},
		"scss": {
			"optimize": true,
			"source": "./static/src/scss/**/*.scss"
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "npx parcel watch ./static/src/main.js ./static/src/js/**/*.js ./static/src/scss/main.scss .static/src/assets/**/*.png --dist-dir ./static/dist --no-hmr",
		"build": "npx parcel build ./static/src/main.js ./static/src/js/**/*.js ./static/src/scss/main.scss .static/src/assets/**/*.png --dist-dir ./static/dist"
	},
	"keywords": [],
	"author": "",
